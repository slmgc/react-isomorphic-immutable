{
  "private": true,
  "main": "src/server",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "each": "concurrently --raw",
    "upgrade": "npm-upgrade",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon",
    "watch": "npm run each 'npm run watch:client' 'npm run watch:server'",
    "start": "npm i && gulp symlinks && npm run watch",
    "test:all": "mocha src/client/*/",
    "test:actions": "mocha src/client/actions",
    "test:components": "mocha src/client/components",
    "test": "npm run test:components"
  },
  "dependencies": {
    "baobab": "2.3.3",
    "baobab-react": "2.0.0-rc2",
    "bluebird": "3.3.4",
    "lodash.clonedeep": "4.3.2",
    "lodash.isequal": "4.1.3",
    "react": "15.0.0-rc.2",
    "react-dom": "15.0.0-rc.2",
    "react-router": "2.0.1",
    "superagent": "1.8.3"
  },
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "babel-runtime": "6.6.1",
    "body-parser": "1.15.0",
    "bootstrap": "4.0.0-alpha.2",
    "concurrently": "2.0.0",
    "cookie-session": "2.0.0-alpha.1",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "gulp": "3.9.1",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "npm-install-webpack-plugin": "3.0.0",
    "npm-upgrade": "0.6.2",
    "should": "8.3.0",
    "webpack": "1.12.14"
  }
}
